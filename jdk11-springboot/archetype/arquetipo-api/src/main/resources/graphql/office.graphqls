type Query {
	offices(
		query : OfficeQuery
		page : Int
		size : Int
	) : [Office]
}

input OfficeQuery {
	officeCode: String
	city: String
	phone: String
	addressLine1: String
	addressLine2: String
	state: String
	country: String
	postalCode: String
	territory: String
}

type Office {
	officeCode: String
	city: String
	phone: String
	addressLine1: String
	addressLine2: String
	state: String
	country: String
	postalCode: String
	territory: String
}
